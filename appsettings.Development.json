{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },

    "Tiles": {
        "Cache": "C:\\temp\\tiles",
        "Providers": [
            {
                "Id": "osm",
                "Url": "https://tile.openstreetmap.org/{0}/{1}/{2}.png",
                "UserAgent": "Your-user-agent",
                "ContentType": "image/png",
                "UseWebp": false,
                "MinZoom": 1,
                "MaxZoom": 19
            }
        ]
    }

}
